#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/home/Coding /exttrial/pomo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/home/Coding /exttrial/pomo/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../msgpackr-extract@3.0.0/node_modules/msgpackr-extract/bin/download-prebuilds.js" "$@"
else
  exec node  "$basedir/../../../../../msgpackr-extract@3.0.0/node_modules/msgpackr-extract/bin/download-prebuilds.js" "$@"
fi
